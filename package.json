{
    "name": "kyrai",
    "private": true,
    "version": "1.0.0",
    "description": "KYRAI web application showcase",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
        "preview": "vite preview",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
